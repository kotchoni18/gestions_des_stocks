{
    "name": "clicalmani/tonka",
    "type": "project",
    "description": "Tonka Framework.",
    "keywords": ["framework", "tonka"],
    "license": "MIT",
    "require": {
        "clicalmani/console": ">=1.0",
        "clicalmani/cookie": ">=1.0",
        "clicalmani/database": ">=1.0",
        "clicalmani/foundation": ">=2.0.3",
        "clicalmani/psr": ">=1.0",
        "clicalmani/routing": ">=1.0",
        "clicalmani/validation": ">=2.0",
        "clicalmani/xpower": ">=1.2.1",
        "clicalmani/task": ">=1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Entities\\": "database/entities/",
            "Test\\": "test/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
